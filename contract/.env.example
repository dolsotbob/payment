# ========================
# Token & Wallet
# ========================
TOKEN_ADDRESS=             # 테스트 토큰 TEST의 ERC-20 주소
STORE_WALLET=              # 스토어(판매자) 지갑 주소 (예: Brave / MetaMask)
PRIVATE_KEY=               # 스토어 지갑 개인키 (민감정보, 절대 노출 금지)
RPC_URL=                   # 블록체인 RPC 엔드포인트
RECIPIENT=                 # 기본 수령자 주소 (테스트 전송용)
PAYMENT_ADDRESS=           # Payment 컨트랙트 주소
VAULT_ADDRESS=             # Vault 컨트랙트 주소

# ========================
# Governance (Timelock / Proxy)
# ========================
TIMELOCK_ADDRESS=          # TimelockController 컨트랙트 주소
PROXIES=                   # 프록시 주소 리스트 (콤마 구분)
WITHDRAW_RECIPIENT=        # Vault 인출 시 자금 수령자 주소
AMOUNT=                    # 인출 금액 (wei 단위)
SALT=                      # 배포/스케줄링 시 사용할 salt 값
DECIMALS=                  # 토큰 소수점 자리 수 (예: 18)

# ========================
# Payment setConfig
# ========================
CASHBACK_BPS=              # 캐시백 비율 (basis points, 100=1%)
MAXDISCOUNT_BPS=           # 최대 할인 비율 (basis points)
SALT_SETCONFIG=            # setConfig 트랜잭션 salt
# INITIAL_OWNER=           # (옵션) 최초 소유자 주소

# ========================
# Coupon Rule 설정
# ========================
RULE_NFT=                  # 쿠폰으로 사용할 ERC1155 컨트랙트 주소
RULE_ID=                   # 쿠폰 토큰 ID
RULE_DISCOUNT_BPS=         # 쿠폰 할인 비율 (basis points)
RULE_EXPIRES_AT=           # 쿠폰 만료 시간 (0=무기한, UNIX timestamp)
RULE_ENABLED=              # 쿠폰 활성화 여부 (true/false)
RULE_CONSUMABLE=           # 쿠폰 1회 사용 가능 여부 (true/false)
SALT_SETRULE=              # setCouponRule 트랜잭션 salt

# ========================
# Coupon Rule 비활성화
# ========================
DISABLE_NFT=0xCoupon1155   # 비활성화할 쿠폰 NFT 주소
DISABLE_ID=1               # 비활성화할 쿠폰 토큰 ID
SALT_DISABLERULE=payment-disrule-1   # disableCouponRule salt

# ========================
# UUPS Upgrade
# ========================
OLD_IMPL_PAYMENT=          # 기존 Payment 구현 주소 (V1, 다운그레이드 테스트용)
NEW_IMPL_PAYMENT=          # 신규 Payment 구현 주소 (V2 이상)
SALT_UPGRADE=              # upgradeTo 트랜잭션 salt
INIT_SIG=                  # 업그레이드 후 호출할 initializer 함수 signature
INIT_ARGS_JSON=            # initializer 함수 인자 (JSON 형식)

# ========================
# Coupon1155 (ERC-1155 NFT)
# ========================
COUPON1155_BASE_URI=       # 쿠폰 메타데이터 baseURI
COUPON1155_INITIAL_OWNER=  # 쿠폰 컨트랙트 초기 소유자

# 초기 민트 (옵션)
COUPON1155_DO_INITIAL_MINT= # true/false (초기 민트 여부)
COUPON1155_MINT_TO=         # 민트 받을 주소
COUPON1155_MINT_IDS=        # 민트할 쿠폰 ID 목록 (예: "1,2,3")
COUPON1155_MINT_AMOUNTS=    # 민트 수량 목록 (예: "10,20,30")

# 사용자 지갑으로 쿠폰 전송 (옵션)
